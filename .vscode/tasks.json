{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                //"/std:c++latest",
                "-std=c++17",
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                //"-I/opt/homebrew/Cellar/boost/1.88.0",
                "-I /Users/christopherlinder/Library/Mobile Documents/.Trash/boost/boost",
                //"-I/Users/christopherlinder/Library/Mobile Documents/.Trash/boost/boost",
                //"-I/opt/local/libexec/boost/1.76/include",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-lboost_system",
                "-lboost_regex",
                "-L/Users/christopherlinder/Library/Mobile Documents/.Trash/boost/boost/stage/lib"
                //"/I", "/Users/christopherlinder/Library/Mobile Documents/.Trash/boost/boost",
                //"/link", "/Users/christopherlinder/Library/Mobile Documents/.Trash/boost/boost/stage/lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
